<table>
  <tr>
    <th>Name</th>
    <th>Color</th>
    <th>Sex</th>
    <th>Age</th>
    <th>Birth Date</th>
  </tr>
  <% @cats.each do |cat|%>
  <tr>
    <td><%= cat.name %></td>
    <td><%= cat.color %></td>
    <td><%= cat.sex %></td>
    <td><%= cat.age %></td>
    <td><%= cat.birth_date %></td>
  </tr>
  <% end %>
</table>